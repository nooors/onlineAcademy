    "cors": "^2.8.5",
    "express": "^4.18.1",
    "graphql": "^16.5.0",
    "graphql-import-node": "^0.0.5",
    "graphql-playground-middleware-express": "^1.7.23",
    "graphql-tools": "^8.3.0",
    "http": "^0.0.1-security",
    "lodash": "^4.17.21",
    "ncp": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
